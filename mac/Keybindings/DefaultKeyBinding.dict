    /* Modifier keys: start with C-m */
    "^m" = {
        "^ "    = ("insertText:", "\U2423");  /* C-space space */

        "^e"    = ("insertText:", "\U21A9");  /* C-e     return */
        "e"     = ("insertText:", "\U2305");  /* e       enter */

        "^t"    = ("insertText:", "\U21E5");  /* C-t     tab */
