#!/usr/bin/env python

import os

def main():
    paths = {}
    for component in os.getenv('PATH', []).split(':'):
        for executable in os.listdir(component):
            paths.setdefault(executable, [])
            paths[executable].append(os.path.realpath(component))
    for executable in sorted(paths.keys()):
        if len(paths[executable]) > 1:
            print "%s has %d entries:" % (executable, len(paths[executable]))
            for component in paths[executable]:
                print "\t%s" % component

if __name__ == '__main__':
    main()
