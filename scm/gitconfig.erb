#  -*- mode: conf -*-

[user]
    name = Martin Kuehl
    email = martin.kuehl@gmail.com
[github]
    user = mkhl
    token = <%= $config[:github_token] %>
[color]
    ui = auto
[man]
    viewer = ManOpen
[man.ManOpen]
    cmd = openman
[man.Bwana]
    cmd = bwana
[alias]
# The name 'push?' is not supported
    needs-pushing = cherry -v origin/master
    chainsaw = rebase --interactive
    st = status
    co = checkout
    ci = commit
    cp = cherry-pick
    fp = format-patch
    bs = bisect
    np = needs-pushing
